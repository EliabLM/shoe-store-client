{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["warn", 2],
    "linebreak-style": ["warn", "unix"],
    "quotes": ["warn", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off",
    "react/display-name": "off",
    "react/no-unknown-property": "off",
    "no-unreachable": "warn"
  }
}
